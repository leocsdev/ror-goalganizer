<table>
  <thead>
    <tr>
      <th>name</th>
      <th>description</th>
      <th>deadline</th>
      <th>status</th>
      <th>goal id</th>
    </tr>
  </thead>

  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.name  %></td>
        <td><%= task.description  %></td>
        <td><%= task.deadline  %></td>
        <td><%= task.status  %></td>
        <td><%= task.goal_id  %></td>
      </tr>
    <% end %>
  </tbody>
</table>